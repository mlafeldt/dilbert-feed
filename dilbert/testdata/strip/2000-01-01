  if (window.amuDilbert.debug == true) {
    console.log("AMU: ***DEBUG MESSAGES ARE ENABLED***");
    function debug(message, object = '') {
      if (window.amuDilbert.debug) {
        console.debug(message, object);
      }
    }
  } else {
    function debug(message, object) {
      return;
    }
  }
</script>

